{
    "name": "crawAudio.uploadYoutube",
    "version": "1.0.0",
    "main": "index.js",
    "type": "commonjs",
    "author": {
        "name": "Duncan Nguyen",
        "email": "duncan.nguyen.18@gmail.com"
    },
    "license": "MIT",
    "scripts": {
        "prepare": "husky install",
        "db:dev": "mongod --config ./config/mongod.dev.conf",
        "dev": "node ./index.js",
        "start:dev": "concurrently \"yarn run db:dev\" \"yarn run dev\"",
        "db:prod": "mongod --config ./config/mongod.prod.conf",
        "prod": "cross-env NODE_ENV=production node ./index.js",
        "start:prod": "concurrently \"yarn run db:prod\" \"yarn run prod\""
    },
    "devDependencies": {
        "@babel/core": "^7.22.5",
        "@babel/node": "^7.22.5",
        "@babel/preset-env": "^7.22.5",
        "@babel/register": "^7.22.5",
        "concurrently": "^8.2.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.3",
        "lint-staged": "13.1.0",
        "prettier": "^2.8.8"
    },
    "lint-staged": {
        "*.js": "eslint --fix",
        "*.js,*.md,*.json": "prettier --write"
    },
    "dependencies": {
        "axios": "^1.4.0",
        "config": "^3.3.9",
        "dotenv": "^16.3.1",
        "fluent-ffmpeg": "^2.1.2",
        "get-audio-duration": "^4.0.1-beta.0",
        "googleapis": "^120.0.0",
        "jsdom": "^22.1.0",
        "lodash": "^4.17.21",
        "mongoose": "^7.3.1",
        "playwright-chromium": "^1.35.1"
    }
}
